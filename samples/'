#include <curses.h>
#include <term.h>
#include <stdio.h>

void dump(char* str) {
  int idx = 0;
  while (str[idx]) {
  }
  printf("\n");
}

int main(int argc, char** argv) {
  /* from `man curs_terminfo` */
  setterm("xterm");
  char* str = tigetstr("cud");
  printf("%s\n", str);
  char* str2 = tparm(str, 69);
  printf("%s\n", str2);
  printf("done\n");
  return 0;
}
